<h1>Editing movement</h1>

<% form_for(@movement) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :seller_id %><br />
    <%= f.text_field :seller_id %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :reason %><br />
    <%= f.text_field :reason %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.check_box :email %>
  </p>
  <p>
    <%= f.label :dispute %><br />
    <%= f.check_box :dispute %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @movement %> |
<%= link_to 'Back', movements_path %>