<h1>Editing draw</h1>

<% form_for(@draw) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :list %><br />
    <%= f.text_field :list %>
  </p>
  <p>
    <%= f.label :seller_id %><br />
    <%= f.text_field :seller_id %>
  </p>
  <p>
    <%= f.label :login_name %><br />
    <%= f.text_field :login_name %>
