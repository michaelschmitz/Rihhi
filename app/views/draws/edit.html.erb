  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :account_holder %><br />
    <%= f.text_field :account_holder %>
  </p>
  <p>
    <%= f.label :iban %><br />
    <%= f.text_field :iban %>
  </p>
  <p>
    <%= f.label :bic %><br />
    <%= f.text_field :bic %>
  </p>
  <p>
    <%= f.label :balance_now %><br />
    <%= f.text_field :balance_now %>
  </p>
  <p>
    <%= f.label :debited %><br />
    <%= f.text_field :debited %>
  </p>
  <p>
    <%= f.label :difference %><br />
    <%= f.text_field :difference %>
  </p>
  <p>
    <%= f.label :fail %><br />
    <%= f.text_field :fail %>
  </p>
  <p>
