<h1>Listing lastcategories</h1>

<table>
  <tr>
  </tr>

<% for lastcategory in @lastcategories %>
  <tr>
    <td><%= link_to 'Show', lastcategory %></td>
    <td><%= link_to 'Edit', edit_lastcategory_path(lastcategory) %></td>
    <td><%= link_to 'Destroy', lastcategory, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New lastcategory', new_lastcategory_path %>
