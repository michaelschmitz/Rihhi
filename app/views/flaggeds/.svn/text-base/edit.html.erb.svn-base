  <p>
    <%= f.label :email_sent %><br />
    <%= f.text_field :email_sent %>
  </p>
  <p>
    <%= f.label :reason %><br />
    <%= f.text_field :reason %>
  </p>
  <p>
    <%= f.label :deleted %><br />
    <%= f.text_field :deleted %>
  </p>
  <p>
    <%= f.label :changed %><br />
    <%= f.text_field :changed %>
  </p>
  <p>
    <%= f.label :closed %><br />
