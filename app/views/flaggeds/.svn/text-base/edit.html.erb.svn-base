<h1>Editing flagged</h1>

<% form_for(@flagged) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :listing_id %><br />
    <%= f.text_field :listing_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :flagger_id %><br />
