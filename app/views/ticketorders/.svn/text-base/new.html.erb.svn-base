<% @onpage = 1 %>
<%= render :partial => "newticket", :object => @onpage %>
<div style="width: 750px; float:left;">
    <h2 class="text_green">Order a ticket</h2>
    <ul style="margin-bottom: 10px;">
        <li>
            You can order tickets for up to 10 people.
			 
        </li>
    </ul>
	<div style="float: left; clear:left; width: 600px; height: 30px;">
        <h2 class="text_green">Ticket: <%=h @ticketorder.ticket.name %></h2>
    </div>
	
    <div style="float: left; width: 80px; height: 30px;">
        <h2 class="text_green">			
			<% if Date.today <= @ticketorder.ticket.price1_deadline %>
			<%=h @ticketorder.ticket.price1 %>
			<% elsif Date.today <= @ticketorder.ticket.price2_deadline %>
			<%=h @ticketorder.ticket.price2 %>
			<% elsif Date.today <= @ticketorder.ticket.price3_deadline %>
			<%=h @ticketorder.ticket.price3 %>
			<% elsif Date.today <= @ticketorder.ticket.price4_deadline %>
			<%=h @ticketorder.ticket.price4 %>
			<% else %>			
			<%=h @ticketorder.ticket.price5 %>			
			<% end %>

			 Euro</h2>
    </div>

	
