<h1>New deliverable</h1>

<% form_for(@deliverable) do |f| %>
  

  <p>
    <%= f.label :listing_id %><br />
    <%= f.text_field :listing_id %>
  </p>
  <p>
    <%= f.label :order_id %><br />
    <%= f.text_field :order_id %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <!-- should have all information from the listing after this point.-->
  <p>
    <%= f.submit "Zum Haus." %>
  </p>
<% end %>

<%= link_to 'Back', deliverables_path %>
